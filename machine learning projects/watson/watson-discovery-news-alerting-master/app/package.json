    "react-test-renderer": "^15.5",
    "recharts": "^1.0.0-alpha.0",
    "request": "^2.81.0",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "tiny-worker": "^2.1.1",
    "url-loader": "0.5.8",
    "url-search-params-polyfill": "^1.3.0",
    "watson-developer-cloud": "^2.32.1",
    "watson-react-components": "^0.6.9",
    "webpack": "2.6.1",
