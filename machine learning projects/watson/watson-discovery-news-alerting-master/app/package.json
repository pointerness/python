    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "^2.0.3",
    "yarn": "^0.27.5"
  },
  "engines": {
    "node": "8.9.1"
  },
  "scripts": {
    "startdev": "./node_modules/babel-cli/bin/babel-node.js ./scripts/start.js",
    "start-notifier": "yarn run build-notifier && node ./node_modules/babel-cli/bin/babel-node.js ./notifier.js",
    "build-notifier": "./node_modules/babel-cli/bin/babel-node.js ./src/buildNotifierDB.js",
    "start": "node ./node_modules/babel-cli/bin/babel-node.js ./server.js",
    "build": "node ./node_modules/babel-cli/bin/babel-node.js ./scripts/build.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "./node_modules/babel-cli/bin/babel-node.js ./scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-config-react-app": "^1.0.4",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-jsx-a11y": "5.0.3"
  }
}
